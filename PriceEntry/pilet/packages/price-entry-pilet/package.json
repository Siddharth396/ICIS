{
  "name": "price-entry-pilet",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start:subscriber": "yarn fetch-local-cert && yarn subscriber:app-shell && yarn open-cli  https://localhost:8050/mfe/price-entry && pilet debug ./src/index --port 8083 --env",
    "start:authoring": "yarn fetch-local-cert && yarn authoring:app-shell && yarn open-cli https://localhost:8050/mfe/price-entry/authoring && pilet debug ./src/authoring-index --port 8083 --env",
    "start:authoring:feed": "yarn fetch-local-cert && yarn authoring:app-shell && yarn open-cli https://localhost:8050/mfe/price-entry/authoring && pilet debug ./src/authoring-index --port 8083 --feed http://localhost:8090/api/v1/mfe --env",
    "start:subscriber:mock": "yarn open-cli https://localhost:8083/mfe/price-entry && pilet debug ./src/index --port 8083 --env",
    "start:authoring:mock": "yarn open-cli https://localhost:8083/mfe/price-entry && pilet debug ./src/authoring-index --port 8083 --env",
    "start:app-shell-mock": "docker pull artifacts.cha.rbxd.ds/sbcore/sbcore-app-mockbff:latest && docker run -p 4200:4200 -d artifacts.cha.rbxd.ds/sbcore/sbcore-app-mockbff:latest",
    "start:docker": "../../dockerup.sh",
    "start:docker:all": "../../dockerup.sh -all",
    "stop:docker": "docker-compose -f ../../docker-compose-authoring.yml down",
    "build": "pilet build",
    "build:authoring": "pilet build ./src/authoring-index",
    "package": "pilet pack --target pilet-package.tgz",
    "publish:local": "pilet build && pilet pack && pilet publish --url http://localhost:8090/api/v1/mfe --api-key  && rm *.tgz",
    "upgrade:pilet": "pilet upgrade",
    "test:tsc": "tsc",
    "test:unit": "node scripts/test.js",
    "test:lint": "eslint \"src/**/*.{js,tsx,ts}\"",
    "test:lint:fix": "eslint --fix \"src/**/*.{js,tsx,ts}\"",
    "test:unit:tc": "node scripts/test.js --testPathIgnorePatterns='TestData.*' --detectOpenHandles --testResultsProcessor jest-teamcity-reporter --watchAll=false --coverage && echo \"Coverage: $(lcov-total coverage/lcov.info)\"",
    "test": "yarn test:lint && yarn test:tsc && yarn test:unit",
    "test:tc": "yarn test:lint && yarn test:tsc && yarn test:unit:tc",
    "authoring:app-shell": "node ./scripts/switchAppShellType.js authoring",
    "subscriber:app-shell": "node ./scripts/switchAppShellType.js",
    "fetch-local-cert": "node ./scripts/downloadLocalCertAndKey.js"
  },
